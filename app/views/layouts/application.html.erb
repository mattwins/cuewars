<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en" dir="ltr">
<head> 
  <title>Cuewars</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body id="<%= controller.controller_name %>" class="<%= controller.action_name %>">
<div id="wrapper">
<div id="top_strip">
</div>
  <div id="header" class="clearfix">
    <div class="container_16 clearfix"> 
      <div class="grid_4"> 
        <div id="logo"> 
          <h1> 
          <%= link_to 'CueWars', root_path %> 
          </h1> 
          <h2>Let the battle commence</h2> 
        </div>
      </div> 
      <div class="grid_11 prefix_1"> 
      	<div id="nav"> 
          <%= render :partial => '/layouts/shared/main_navigation' %>
    		</div>
      </div>
    </div>
  </div>
  <div id="impact_banner"> 
  	<div class="container_16 clearfix"> 
  		<div class="grid_7"> 
  			<div id="mission" class="clearfix"> 
  			  <h1 style="font-size : 60px;">Big ass statement here</h1>
  			</div><!-- // mission --> 
  			<div id="options"> 
  				<a href="#" id="lm">Sign Up</a> 
  			</div><!-- // options --> 
  		</div> 
  		<div class="grid_9"> 
  		  <%= yield :impact_banner %>
  		</div>
  	</div> 
  </div>
  
  <div id="content"> 
  	<div class="container_16 clearfix">
      <%= yield %>
    </div>
  </div>
</div>
</body>
</html>
